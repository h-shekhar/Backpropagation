    "import numpy as np \n",
    "input = np.linspace(-5, 5, 100)\n",
    "\n",
    "def sigmoid(x):  \n",
    "    return 1/(1+np.exp(-x))\n",
    "\n",
    "from matplotlib import pyplot as plt  \n",
    "plt.plot(input, sigmoid(input), c=\"r\")  "
